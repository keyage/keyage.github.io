{
  "name": "qwikey",
  "devDependencies": {
    "@builder.io/qwik": "^1.18.0",
    "@builder.io/qwik-city": "^1.18.0",
    "@qwikest/icons": "^0.0.13",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "25.0.3",
    "globals": "16.5.0",
    "oxlint": "^0.17.0",
    "oxlint-tsgolint": "^0.10.0",
    "oxfmt": "^0.17.0",
    "tailwindcss": "^4.1.18",
    "typescript": "5.9.3",
    "typescript-eslint": "8.51.0",
    "typescript-plugin-css-modules": "latest",
    "undici": "^7.16.0",
    "vite": "7.3.0",
    "vite-tsconfig-paths": "^6.0.3"
  },
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "qwik check-client src dist && vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "oxfmt .",
    "fmt.check": "oxfmt --check .",
    "lint": "oxlint --react-plugin --jsx-a11y-plugin --import-plugin",
    "lint.fix": "oxlint --fix-dangerously --react-plugin --jsx-a11y-plugin --import-plugin",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "type": "module",
  "pnpm": {
    "overrides": {
      "glob": "^10.0.0",
      "inflight": "^2.0.0"
    }
  }
}
